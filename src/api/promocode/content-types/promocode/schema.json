      "unique": true
    },
    "title": {
      "type": "string"
    },
    "discountType": {
      "type": "enumeration",
      "enum": [
        "percentage,",
        "fixed_amount,",
        "free_delivery"
      ]
    },
    "discountValue": {
      "type": "decimal",
      "required": true
    },
    "minOrderAmount": {
      "type": "decimal",
